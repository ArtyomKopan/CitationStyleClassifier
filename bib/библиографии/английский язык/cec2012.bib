@InProceedings{Aleti:2012:CEC,
  title     = {Adaptive Range Parameter Control},
  author    = {Aldeida Aleti and Irene Moser and Sanaz Mostaghim},
  pages     = {2405--2412},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
  year      = {2012},
  editor    = {Xiaodong Li},
