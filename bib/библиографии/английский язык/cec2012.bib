@InProceedings{Qiu:2012:CEC,
  title     = {A Geometrically Faithful Memetic Algorithm for Searching Sparse Representations of {EEG} Signals},
  author    = {Jun-Wei Qiu and John K. Zao and Yu-Hsiang Chou},
