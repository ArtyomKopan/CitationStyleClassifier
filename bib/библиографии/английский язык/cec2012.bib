@InProceedings{Duma:2012:CEC,
  title     = {Classification with Missing Data using Multi-Layered Artificial Immune Systems},
  author    = {Mlungisi Duma and Bhekisipho Twala and Tshilidzi Marwala and Fulufhelo Nelwamondo},
  pages     = {1687--1694},
