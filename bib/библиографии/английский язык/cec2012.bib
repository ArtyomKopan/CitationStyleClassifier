@InProceedings{Lattarulo:2012:CEC,
  title     = {A Preliminary Study of a new Multi-objective Optimization Algorithm},
  author    = {Valerio Lattarulo and Geoffrey T. Parks},
  pages     = {1793--1800},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
