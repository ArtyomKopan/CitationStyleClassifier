@InProceedings{Liu:2012:CECh,
  title     = {Survival Analysis of Gene Expression Data Using {PSO} Based Radial Basis Function Networks},
  author    = {Wenmin Liu and Zhen Ji and Zexuan Zhu and Shan He},
  pages     = {3095--3099},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
  year      = {2012},
  editor    = {Xiaodong Li},
