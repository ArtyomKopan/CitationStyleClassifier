@InProceedings{Veeramachaneni:2012:CEC,
  title     = {Optimizing Energy Output and Layout Costs for Large Wind Farms using Particle Swarm Optimization},
  author    = {Kalyan Veeramachaneni and Markus Wagner and Una-May O'Reilly and Frank Neumann},
  pages     = {3540--3546},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
