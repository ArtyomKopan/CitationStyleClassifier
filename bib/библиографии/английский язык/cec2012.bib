}

@InProceedings{Ou:2012:CEC,
  title     = {Immune Memory with Associativity: Perspectives on Dynamical Systems},
  author    = {Chung-Ming Ou and C.R. Ou},
  pages     = {928--933},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
