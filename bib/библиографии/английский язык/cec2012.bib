@InProceedings{Brabazon:2012:CEC,
  title     = {Optimal Patent Design: An Agent-based Approach},
  author    = {Anthony Brabazon and Arlindo Silva and Michael O'Neill},
  pages     = {1102--1109},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
  year      = {2012},
