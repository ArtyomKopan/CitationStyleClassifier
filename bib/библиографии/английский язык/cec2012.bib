@InProceedings{Gao:2012:CEC,
  title     = {A composite heuristic for the no-wait flow shop scheduling},
  author    = {Kaizhou Gao and Ponnuthurai Nagaratnam Suganthan and Zhenqiang Bao},
  pages     = {1125--1129},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
