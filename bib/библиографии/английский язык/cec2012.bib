  CEC 2012 - A joint meeting of the IEEE, the EPS and the IET.},
}


% Special Session: Artificial Bee Colony Algorithm 1
@InProceedings{Hancer:2012:CEC,
  title     = {Artificial Bee Colony Based Image Clustering},
  author    = {Emrah Hancer and Celal Ozturk and Dervis Karaboga},
  pages     = {2926--2930},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
