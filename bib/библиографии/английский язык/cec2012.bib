}

@InProceedings{Sekanina:2012:CEC,
  title     = {Two-Step Evolution of Polymorphic Circuits for Image Multi-Filtering},
  author    = {Lukas Sekanina and Vojtech Salajka and Zdenek Vasicek},
  pages     = {1481--1488},
