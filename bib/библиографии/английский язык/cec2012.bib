@InProceedings{Sengupta:2012:CECa,
  title     = {An Adaptive Memetic Algorithm Using a Synergy of Differential Evolution and Learning Automata},
  author    = {Abhronil Sengupta and Tathagata Chakraborti and Amit Konar and Eunjin Kim and Atulya K. Nagar},
  pages     = {2436--2443},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
