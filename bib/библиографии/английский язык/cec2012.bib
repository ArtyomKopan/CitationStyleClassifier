@InProceedings{Santana:2012:CECa,
  title     = {An analysis of the use of probabilistic modelling for synaptic connectivity prediction from genomic data},
  author    = {Roberto Santana and Alexander Mendiburu and Jose A. Lozano},
  pages     = {3495--3502},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
  year      = {2012},
