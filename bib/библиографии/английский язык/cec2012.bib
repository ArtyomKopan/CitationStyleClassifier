@InProceedings{Sharma:2012:CEC,
  title     = {Artificial Bee Colony with Mean Mutation Operator for Better Exploitation},
  author    = {Tarun Kumar Sharma and Millie Pant and Jagdish Chand Bansal},
  pages     = {3045--3051},
  booktitle = {Proceedings of the 2012 IEEE Congress on Evolutionary Computation},
