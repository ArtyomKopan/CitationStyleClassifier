@inproceedings{Golemo:2015:GECCOcomp,
 author = {Florian Golemo and Marjeta Markovic and Jasper Schoormans and Peter {De Jonge} and Maik Couwenberg and Evert Haasdijk},
 title = {Simulating Morphological Evolution in Large Robot Populations},
 booktitle = {GECCO 2015 Evolving Collective Behaviors in Robotics (ECBR'15) Workshop},
