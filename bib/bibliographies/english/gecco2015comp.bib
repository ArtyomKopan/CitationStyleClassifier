 address = {Madrid, Spain},
 doi = {http://dx.doi.org/10.1145/2739482.2768490},
 doi = {doi:10.1145/2739482.2768490},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {In this work, the canonical distributed embodied evolution algorithm used to solve a collective task in which a team of Micro Aerial Vehicles (MAVs) has to do surveillance in an indoor area. In order to efficiently survey the arena, the MAVs need to locate themselves and keep track of the recent covered areas and to share this information with other robots. This self-localization is performed using an IMU and a camera by means of artificial landmarks that can be sensed using the on-board camera and the position of other MAV in sight. The accuracy in the location of each MAV arises as a dynamic parameter and has been included as part of the problem to solve. Therefore, the collective control system is in charge of organizing the MAVs in order to increase the surveillance efficiency which is also subject to maintain a suitable accuracy for each of the MAVs.},
 notes = {Also known as \cite{2768490}
Distributed at GECCO-2015.},
 }

@inproceedings{Heinerman:2015:GECCOcomp,
