Distributed at GECCO-2015.},
 }

@inproceedings{Trueba:2015:GECCOcomp,
 author = {Pedro Trueba and Abraham Prieto and Francisco Bellas and Richard J. Duro},
 title = {Embodied Evolution for Collective Indoor Surveillance and Location},
 booktitle = {GECCO 2015 Evolving Collective Behaviors in Robotics (ECBR'15) Workshop},
 year = {2015},
 editor = {Abraham Prieto and Nicolas Bredeche and Evert Haasdijk},
  isbn13 = {978-1-4503-3488-4},
 keywords = {},
