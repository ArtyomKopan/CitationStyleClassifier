@inproceedings{Bredeche:2015:GECCOcomp,
 author = {Nicolas Bredeche and Stephane Doncieux and Jean-Baptiste Mouret},
 title = {Tutorial on Evolutionary Robotics},
 booktitle = {GECCO 2015 Introductory Tutorials},
 year = {2015},
 editor = {Anabela Simoes},
  isbn13 = {978-1-4503-3488-4},
 keywords = {},
 pages = {231--261},
