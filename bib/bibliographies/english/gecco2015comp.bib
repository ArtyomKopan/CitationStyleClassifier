@inproceedings{Shehu:2015:GECCOcomp,
 author = {Amarda Shehu and Kenneth {De Jong}},
 title = {Evolutionary Algorithms for Protein Structure Modeling},
 booktitle = {GECCO 2015 Advanced Tutorials},
 year = {2015},
 editor = {Anabela Simoes},
  isbn13 = {978-1-4503-3488-4},
 keywords = {},
