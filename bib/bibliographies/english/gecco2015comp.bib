 organisation = {SIGEVO},
 address = {Madrid, Spain},
 doi = {http://dx.doi.org/10.1145/2739482.2764691},
 doi = {doi:10.1145/2739482.2764691},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {We integrate a Denoising Autoencoder (DAE) into an Estimation of Distribution Algorithm (EDA) and evaluate the performance of DAE-EDA on several combinatorial optimization problems. We asses the number of fitness evaluations and the required CPU times. Compared to the state-of-the-art Bayesian Optimization Algorithm (BOA), DAE-EDA needs more fitness evaluations, but is considerably faster, sometimes by orders of magnitude. These results show that DAEs can be useful tools for problems with low but non-negligible fitness evaluation costs.},
