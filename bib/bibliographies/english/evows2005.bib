@InProceedings{karaman:evows05,
  author = {Ayd\i{}n Karaman and \c{S}ima Uyar and G\"{u}l\c{s}en Eryi\u{g}it},
  title = {The Memory Indexing Evolutionary Algorithm for Dynamic Environments},
