  ISSN =        {0302-9743},
  doi =         {},
  url =         {},
  size =        {},
  abstract =    {Experiments are described which use genetic
  algorithms operating on the parameter settings of an FM synthesiser,
  with the aim of mimicking known synthesized sounds. The work is
  considered as a precursor to the development of synthesis plug-ins
  using evolution directed by a user. Attention is focused on the
  fitness functions used to drive the evolution: the main result is
  that a composite fitness function D based on a combination of
  perceptual measures, spectral analysis, and low-level
  sample-by-sample comparison D drives more successful evolution than
  fitness functions which use only one of these types of criterion.},
  notes =       {EvoWorkshops2005}
}

@InProceedings{urbano:evows05,
