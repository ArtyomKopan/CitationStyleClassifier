@inproceedings(Ono:2008:cec,
  author = "Satoshi Ono and Kensuke Morinaga and  Shigeru Nakayama",
  title = "Two-Dimensional Barcode Decoration Based on Real-Coded Genetic Algorithm",
  booktitle = "2008 IEEE World Congress on Computational Intelligence",
  year = 2008,
  editor = "Jun Wang",
  pages = {--},
  address = "Hong Kong",
  month = "1-6 June",
  organization ="IEEE Computational Intelligence Society",
  publisher = "IEEE Press",
  note = {},
