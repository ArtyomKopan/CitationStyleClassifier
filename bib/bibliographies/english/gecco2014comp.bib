 doi = {doi:10.1145/2598394.2598459},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Dimensionality reduction is an important problem class in machine learning and data mining, as the dimensionality of data sets is steadily increasing. This work is a contribution in the line of research on iterative unsupervised kernel regression (UKR), a class of methods for dimensionality reduction that employ regression methods to find low-dimensional representations of high-dimensional patterns. We introduce a hybrid optimisation approach of iteratively constructing a solution and performing gradient descent in the data space reconstruction error (DSRE). Further, we introduce a variable kernel function that increases the flexibility of UKR learning. The variable kernel function increases the model capacity, but introduces new parameters that have to be tuned.},
 notes = {Also known as \cite{2598459}
Distributed at GECCO-2014.},
 }

@inproceedings{MoradiKordmahalleh:2014:GECCOcomp,
