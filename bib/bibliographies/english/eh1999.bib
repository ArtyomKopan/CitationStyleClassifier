publisher	= {IEEE Computer Society},
note		= {},
%email		= {},
keywords	= {},
ISBN		= {0-7695-0256-3},
url		= {},
size		= {},
abstract	= {Our previous work focused on the synthesis of sequential circuits based on a partial input/output sequence. As the behavioural description of the target circuit is not known the correctness of the result can not be verified. This paper proposes a method which increases the correctness percentage of the finite-state machine (FSM) synthesis using multiple partial input/output sequences. The synthesizer is based on Genetic Algorithm. The experimental results show that the correctness percentage can be increased to 100% by increasing of the number of input/output sequences.},
notes		= {EH1999 http://cism.jpl.nasa.gov/events/nasa_eh/}
)
