@InProceedings{tavaresA:2005:icannga,
  author = "Jorge Tavares and Francisco B. Pereira and Ernesto Costa",
  title = "Golomb Rulers: Experiments with Marks Representation",
  pages = "235--238",
  booktitle = "Adaptive and Natural Computing Algorithms",
  year = "2005",
  editor = "Bernardete Ribeiro and Rudof F. Albrecht and Andrej
            Dobnikar and David W. Pearson and Nigel C. Steele",
  series = "Springer Computer Series",
  publisher = "Springer",
  ISBN = "3-211-24934-6",
  address = 	{Coimbra, Portugal},
  month = 	{21-23 March},
  abstract = {We present a set of experiments regarding an
  evolutionary algorithm designed to efficiently search for Optimal
  Golomb Rulers. The approach uses a binary representation to codify
  the marks contained in a ruler and relies on standard genetic
  operators. Furthermore, during evaluation, an insertion and
  correction procedure is implemented in order to improve the
  algorithm performance. This method is successful in quickly
  identifying good solutions, outperforming previous evolutionary
  approaches by discovering optimal and near-optimal Golomb Rulers.},
  notes =	{http://icannga05.dei.uc.pt/},
}

@InProceedings{tavaresB:2005:icannga, 
  author = "Jorge Tavares and Tiago Leitao and Francisco B. Pereira and Ernesto Costa",
  title = "Evolving Segments Length in Golomb Rulers",
  pages = "239--242",
  booktitle = "Adaptive and Natural Computing Algorithms",
  year = "2005",
  editor = "Bernardete Ribeiro and Rudof F. Albrecht and Andrej
            Dobnikar and David W. Pearson and Nigel C. Steele",
  series = "Springer Computer Series",
  publisher = "Springer",
  ISBN = "3-211-24934-6",
  address = 	{Coimbra, Portugal},
  month = 	{21-23 March},
  abstract = {An evolutionary algorithm based on Random Keys to
  represent Golomb Rulers segments, has been found to be a reliable
  option for finding Optimal Golomb Rulers in a short amount of time,
  when comparing with standard methods. This paper presents a modified
  version of this evolutionary algorithm where the maximum segment
  length for a Golomb Ruler is also part of the evolutionary
  process. Attained experimental results shows us that this alteration
  does not seems to provide significant benefits to the static version
  of the algorithm.},

  notes =	{http://icannga05.dei.uc.pt/},
}

