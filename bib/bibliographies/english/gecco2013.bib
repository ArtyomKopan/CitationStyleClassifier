 organisation = {SIGEVO},
 address = {Amsterdam, The Netherlands},
 doi = {doi:10.1145/2463372.2463455},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Multi-objective optimisation problems are usually solved with evolutionary algorithms when the objective functions are cheap to compute, or with surrogate-based optimisers otherwise. In the latter case, the objective functions are modelled with powerful non-linear model learners such as Gaussian Processes or Support Vector Machines, for which the training time can be prohibitively large when dealing with optimisation problems with moderately expensive objective functions. In this paper, we investigate the use of model trees as an alternative kind of model, providing a good compromise between high expressiveness and low training time. We propose a fast surrogate-based optimiser exploiting the structure of model trees for candidate selection. The empirical results show the promise of the approach for problems on which classical surrogate-based optimisers are painfully slow.},
