@inproceedings{Wagner:2013:GECCO,
 author = {Markus Wagner and Frank Neumann},
 title = {A fast approximation-guided evolutionary multi-objective algorithm},
 booktitle = {GECCO '13: Proceeding of the fifteenth annual conference on Genetic and evolutionary computation conference},
