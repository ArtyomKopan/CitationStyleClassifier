@inproceedings{Chou:2013:GECCO,
 author = {Chih-Yuan Chou and Tian-Li Yu},
 title = {Using representative strategies for finding nash equilibria},
 booktitle = {GECCO '13: Proceeding of the fifteenth annual conference on Genetic and evolutionary computation conference},
