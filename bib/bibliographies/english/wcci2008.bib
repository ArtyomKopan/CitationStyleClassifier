)

@inproceedings(Chamorro-Mart&#237;nez:2008:fuzz,
  author = "J. Chamorro-Mart&#237;nez and D. Sanchez and  J. M. Soto-Hidalgo",
  title = "A Novel Histogram Definition for Fuzzy Colour Spaces",
  booktitle = "2008 IEEE World Congress on Computational Intelligence",
  year = 2008,
  editor = "Jun Wang",
  pages = {--},
  address = "Hong Kong",
  month = "1-6 June",
  organization ="IEEE Computational Intelligence Society",
  publisher = "IEEE Press",
  note = {},
  ISBN13 = "978-1-4244-1819-0",
  file = {FS0491.pdf},
  url = {},
  size = {},
  abstract =	{In this paper we introduce fuzzy naturals-based histograms on fuzzy colour spaces. In our approach, histograms are fuzzy probability distributions on a fuzzy colour space, where the fuzzy probability is calculated as the quotient between a fuzzy natural number and the number of pixels in the image, the former being a fuzzy (non-scalar) cardinality of a fuzzy set. This approach to histograms avoids the well-known disadvantages of the ordinary sigma-count as an estimation of the probability.We illustrate the potential application of the proposal by applying it to the problem of dominant colour selection. },
  notes =	{WCCI 2008 - A joint meeting of the IEEE, the INNS, the EPS and the IET.},
)

