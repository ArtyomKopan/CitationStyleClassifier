)

@inproceedings(Woodside:2008:ijcnn,
  author = "Joseph M. Woodside  ",
  title = "Neuro-Fuzzy CBR Hybridization: Healthcare Application",
  booktitle = "2008 IEEE World Congress on Computational Intelligence",
  year = 2008,
