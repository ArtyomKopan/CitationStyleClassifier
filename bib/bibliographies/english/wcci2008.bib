  file = {FS0356.pdf},
  url = {},
  size = {},
  abstract =	{This paper focus on obtaining a qualitative dynamic model based on real world data taken from a real world qualitative system: the day to day behavior of purse seine fishing fleet skippers. The model is based on a Dynamic Cognitive Mapping approach (Rule Based Fuzzy Cognitive Maps - RB-FCM) where several developments had to be made in order to obtain a workable system. Most changes were due to timing issues, which are essential in the study of System Dynamics but have traditionally been avoided in most Dynamic Cognitive Maps modelling approaches.   },
  keywords = { Rule Based Fuzzy Cognitive Maps, Purse-Seine Fishing, Modelling of dynamic qualitative systems.},
  notes =	{WCCI 2008 - A joint meeting of the IEEE, the INNS, the EPS and the IET.},
)

@inproceedings(Jasinevicius:2008:fuzz,
