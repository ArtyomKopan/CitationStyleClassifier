@inproceedings(Huang7:2008:ijcnn,
  author = "Jian Huang and Xiaoming Chen and P C Yuen and Jun Zhang and W S Chen and J H Lai",
  title = "Kernel Parameter Optimization for Kernel-based LDA Methods",
  booktitle = "2008 IEEE World Congress on Computational Intelligence",
  year = 2008,
